{
  "name": "slimmvc/website",
  "description": "a custom mvc framework to serve crud app",
  "scripts": {
    "serve": "php -S 127.0.0.1:8001 -t public public/index.php"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app",
      "Slimmvc\\": "slimmvc"
    },
    "files": [
      "slimmvc/helpers.php"
    ]
  },
  "config": {
    "process-timeout": 0
  },
  "require": {
    "filp/whoops": "^2.1",
    "vlucas/phpdotenv": "^5.2",
    "league/flysystem": "^3.19.0",
    "firebase/php-jwt": "v6.9.0"
  }
}